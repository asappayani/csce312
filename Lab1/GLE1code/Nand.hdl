/**
 * Nand gate:
 * if (a and b) out = 0, else out = 1
 */

CHIP Nand {
    IN a, b;
    OUT out;
    
    PARTS:
    //// Task: Build a NAND chip using only NOR gates.
    Nor(a=a, b=a, out=nota);
    Nor(a=b, b=b, out=notb);
    Nor(a=nota, b=notb, out=tmp);
    Nor(a=tmp, b=tmp, out=out);
}